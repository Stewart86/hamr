{
  "name": "URL",
  "description": "Open URLs in browser",
  "icon": "open_in_browser",
  "handler": {
    "type": "stdio",
    "command": "python3 handler.py"
  },
  "match": {
    "patterns": [
      "^[a-zA-Z0-9][a-zA-Z0-9-]*\\.[a-zA-Z]{2,}"
    ],
    "priority": 90
  },
  "supportedPlatforms": [
    "niri",
    "hyprland",
    "macos",
    "windows"
  ]
}